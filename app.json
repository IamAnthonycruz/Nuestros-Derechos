{
  "expo": {
    "name": "Nuestros Derechos",
    "slug": "NuestrosDerechos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/LogoFinal.png",
    "scheme": "nuestrosderechos",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nuestrosderechos",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.nuestrosderechos",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LogoFinal.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/LogoFinal.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/LogoFinal.png",
          "imageWidth": 1024,
          "imageHeight": 1024,
          "resizeMode": "cover"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2e8b0c92-8337-4f8f-9969-2bbfc2b1ae47"
      }
    }
  }
}
